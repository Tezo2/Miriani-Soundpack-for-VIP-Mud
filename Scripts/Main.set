  #If {%Defined(ActiveClasses)=1} {
   #ForAll {@ActiveClasses} {
    #Class {%i} Disable;
    #Exec {#UnVar {%i}}
   };
   #UnVar ActiveClasses
  };
  #Var Main %1;
  #Class Main Enable;
  #Class MainOff Disable
 } {
  #If {%Length(%2)>0} {
   #ForAll {%2} {
