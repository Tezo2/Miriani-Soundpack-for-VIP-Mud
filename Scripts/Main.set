#GAlias login {
 #If {%Defined(Main)=1 and %Defined(LoadedSoundpackVersion)=1} {
  #If {%Length(%CharInfo(Name))>0} {
   #If {%IfWord(Ready,%1,|)=1 or (%IfWord(%LastLine(1),Username:,%CRLF)=1 and %Defined(LoadTmp1)=0 and %Defined(LoadTmp2)=0)} {
    #Exec {>%CharInfo(Name)};
    #If {%Length(%CharInfo(Password))>0} {#Exec {>%CharInfo(Password)}};
    #UnVar AutoLogin;
    #GUnVar AutoLogin
   } {#Var AutoLogin 1}
