 #If {%NumWords(@CombatPoints,|)<>2} {
  #If {%IsNumber(@CombatPoints)=1} {#Var CombatPoints {@CombatPoints|0}} {#Var CombatPoints {%4|%5}}
 };
 #Math DevicesTmp2 {%2-%Word(@LicensePoints,|,1)};
