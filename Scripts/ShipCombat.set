}
}} {Main|Critical}
#Trigger {*An escape pod suddenly lands nearby.} {msplay Ship\Misc\EscapePodDescend@Ext} {Main}
#TRIGGER {You hear a soft mechanical whine as the fighter bay's hatch *s.} {ExpPipe {%0} {The fighter bay's hatch %1s.} {};
#sub @ExpString;
